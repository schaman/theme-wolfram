      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body class="preloader global-color_sun" data-spy="scroll" data-target=".navbar">

    <!-- Background Pattern -->
    <div class="bg-pattern bg-pattern_vertical"></div>
    <!-- Backstretch Carousel -->
    <div class="backstretch-carousel"></div>

    <!-- _Preview Setup -->
    <div class="setup__btn hidden-xs">
      <i class="fa fa-flask"></i>
    </div>
    <div class="setup__block">
      <div class="setup-block__close"><i class="fa fa-times"></i></div>
      <div class="setup-block__title">Theme Color Options</div>
      <ul class="setup-block__color-options color-options">
        <li class="color-options_white" title="White" data-color="white"></li>
        <li class="color-options_sun" title="Sun" data-color="sun"></li>
        <li class="color-options_confetti" title="Confetti" data-color="confetti"></li>
        <li class="color-options_picton-blue" title="Picton Blue" data-color="picton-blue"></li>
        <li class="color-options_yellow" title="Yellow" data-color="yellow"></li>
        <li class="color-options_bittersweet" title="Bittersweet" data-color="bittersweet"></li>
      </ul>
      <hr />
      <div class="setup-block__title">Navbar Color Options</div>
      <ul class="setup-block__color-options navbar-options">
        <li class="navbar-options_default" title="Defaul" data-navbar="default" data-faded="0"></li>
        <li class="navbar-options_default-faded" title="Default Faded" data-navbar="default" data-faded="1"></li>
        <li class="navbar-options_inverse-faded" title="Inverse Faded" data-navbar="inverse" data-faded="1"></li>
        <li class="navbar-options_inverse" title="Inverse" data-navbar="inverse" data-faded="0"></li>
      </ul>
      <hr />
      <div class="setup-block__title">Menu Position</div>
      <ul class="setup-block__icons menu-options">
        <li class="menu-options_topbar" title="Topbar"><i class="fa fa-arrow-up fa-fw"></i></li>
        <li class="menu-options_sidebar" title="Sidebar"><i class="fa fa-arrow-right fa-fw"></i></li>
      </ul>
      <hr />
      <div class="setup-block__title">Background patterns</div>
      <ul class="setup-block__icons background-options">
        <li class="pattern-options_vertical-stripes" title="Vertical Stripes" data-pattern="vertical"><i class="fa fa-bars fa-rotate-90 fa-fw"></i></li>
        <li class="pattern-options_horizontal-stripes" title="Horizontal Stripes" data-pattern="horizontal"><i class="fa fa-bars fa-fw"></i></li>
        <li class="pattern-options_solid-color" title="Solid color" data-pattern="no"><i class="fa fa-square fa-fw"></i></li>
      </ul>
    </div>

    <!-- Sidebar Menu -->
    <div class="sidebar-btn hidden"><i class="fa fa-bars fa-2x"></i></div>
    <div class="sidebar-overlay hidden"></div>
    <div class="sidebar sidebar-menu hidden">
      <div class="sidebar-menu__title">Wolfram 1.0</div>
      <ul class="sidebar-menu__ul">
        <li><a href="#welcome"><i class="fa fa-home fa-fw"></i> Welcome</a></li>
        <li><a href="#about"><i class="fa fa-info-circle fa-fw"></i> About</a></li>
        <li><a href="#portfolio"><i class="fa fa-image fa-fw"></i> Portfolio</a></li>
        <li><a href="#pricing"><i class="fa fa-money fa-fw"></i> Pricing</a></li>
        <li><a href="#team"><i class="fa fa-users fa-fw"></i> Team</a></li>
        <li><a href="#features"><i class="fa fa-asterisk fa-fw"></i> Features</a></li>
        <li><a href="#contact-us"><i class="fa fa-envelope fa-fw"></i> Contact Us 1</a></li>
      </ul>
    </div>

    <!-- Top Menu -->
    <div class="navbar navbar-fixed-top navbar-inverse navbar-faded navbar-transparent" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">{{ company }}</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="active"><a href="#welcome">{{ title }}</a></li>
            <li><a href="#features">{{ menu.about }}</a></li>
            <li><a href="#portfolio">{{ menu.review }}</a></li>
            <li><a href="#team">{{ menu.author }}</a></li>
            <li><a href="#contact-us">{{ menu.sign }}</a></li>
            {#<li><a href="#about">About</a></li>
            <li><a href="#pricing">Pricing</a></li>#}
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

