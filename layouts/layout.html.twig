    <link rel="shortcut icon" href="ico/favicon.ico">
    <title>Wolfram - Customizable One Page Template</title>

    <!-- Custom Styles -->
    <link href="{{ 'css/style.css' | asset_url }}" rel="stylesheet">

    <!-- Third Party Styles -->
    <link href="{{ 'fonts/font-awesome-4.2.0/css/font-awesome.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'css/animate.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'css/lightbox.css' | asset_url }}" rel="stylesheet">

    <!-- Google Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Oswald:400,700' rel='stylesheet' type='text/css'>

    <!-- Preloader -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="{{ 'js/queryloader2.js' | asset_url }}"></script>
    <script src="{{ 'js/init_preloader.js' | asset_url }}"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
