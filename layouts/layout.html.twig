<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="ico/favicon.ico">
    <title>{{ title }}</title>

    <!-- Custom Styles -->
    <link href="{{ 'css/style.css' | asset_url }}" rel="stylesheet">

    <!-- Third Party Styles -->
    <link href="{{ 'fonts/font-awesome-4.2.0/css/font-awesome.min.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'css/animate.css' | asset_url }}" rel="stylesheet">
    <link href="{{ 'css/lightbox.css' | asset_url }}" rel="stylesheet">

    <!-- Google Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Oswald:400,700' rel='stylesheet' type='text/css'>

    <!-- Preloader -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="{{ 'js/queryloader2.js' | asset_url }}"></script>
    <script src="{{ 'js/init_preloader.js' | asset_url }}"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body class="preloader global-color_sun" data-spy="scroll" data-target=".navbar">

    <!-- Background Pattern -->
    <div class="bg-pattern bg-pattern_vertical"></div>
    <!-- Backstretch Carousel -->
    <div class="backstretch-carousel"></div>

    <!-- Top Menu -->
    <div class="navbar navbar-fixed-top navbar-inverse navbar-faded navbar-transparent" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Wolfram 1.0</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="active"><a href="#welcome">Welcome</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#portfolio">Portfolio</a></li>
            <li><a href="#pricing">Pricing</a></li>
            <li><a href="#team">Team</a></li>
            <li><a href="#features">Features</a></li>
            <li><a href="#contact-us">Contact Us</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

    {% block body %}
      webinar
        -- about
        features
        reviews (projects)
        authors (team)
        -- pricing
        contact
    {% endblock %}

    <!-- JavaScript -->
    <script src="{{ 'js/jquery.backstretch.min.js' | asset_url }}"></script>
    <script src="{{ 'js/waypoints.min.js' | asset_url }}"></script>
    <script src="{{ 'js/bootstrap.min.js' | asset_url }}"></script>
    <script src="{{ 'js/lightbox.min.js' | asset_url }}"></script>
    <script src="{{ 'js/scrolltopcontrol.js' | asset_url }}"></script>
    <script src="{{ 'js/custom.js' | asset_url }}"></script>
    <script src="{{ 'js/_preview.js' | asset_url }}"></script>
  </body>
</html>